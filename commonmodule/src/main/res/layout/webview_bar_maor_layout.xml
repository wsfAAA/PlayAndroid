<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dp_200"
    android:layout_height="wrap_content"
    android:layout_gravity="right"
    android:background="@color/color_ffffff"
    android:orientation="vertical">

    <TextView
        android:id="@+id/m_tv_web_share"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/dp_5"
        android:paddingLeft="@dimen/dp_20"
        android:paddingTop="@dimen/dp_10"
        android:text="@string/web_share"
        android:textColor="@color/color_545454"
        android:textSize="@dimen/sp_15" />

    <TextView
        android:id="@+id/m_tv_web_collect"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/dp_5"
        android:paddingLeft="@dimen/dp_20"
        android:paddingTop="@dimen/dp_10"
        android:text="@string/web_collect"
        android:textColor="@color/color_545454"
        android:textSize="@dimen/sp_15" />


    <TextView
        android:id="@+id/m_tv_web_bs_open"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/dp_5"
        android:paddingLeft="@dimen/dp_20"
        android:paddingTop="@dimen/dp_10"
        android:text="@string/app_bs_open"
        android:textColor="@color/color_545454"
        android:textSize="@dimen/sp_15" />
</LinearLayout>